<presentationmodel>
	<stylerule element="*" decorator="__Codeword" description="Keywords">
		<foreground>#A00000</foreground>
		<background>#FFFFFF</background>
		<bold />
	</stylerule>
	
	<stylerule element="*" decorator="__Name" description="Names">
		<foreground>#00A000</foreground>
		<background>#F0F0F0</background>
		<bold />
	</stylerule>	
	
	<stylerule element="*" decorator="__EnumMember" description="Enumeration Members">
		<foreground>#FF00A0</foreground>
		<background>#FFFFF0</background>
		<italic />
	</stylerule>	
	
	<stylerule element="chameleon.core.expression.RegularLiteral" decorator="__ALL" description="Literals">
		<foreground>#0000FF</foreground>
		<background>#FFFFFF</background>
	</stylerule>	
	
	<stylerule element="chameleon.core.method.RegularMethod" decorator="__ALL" description="Methods">
		<foldable />
	</stylerule>
	<stylerule element="chameleon.core.method.Constructor" decorator="__ALL" description="Constructors">
		<foldable />
	</stylerule>	
	
	<stylerule element="*" decorator="__Type" description="Types">
		<foreground>#000000</foreground>
		<background>#FFFFFF</background>
		<italic />
	</stylerule>

	<outline>
		<element default="visible" description="Types">CSharpClass</element>
		<element default="visible" description="Inner Types">CSharpInnerClass</element>
		<element default="visible" description="Classes">Constructor</element>
		<element default="hidden" description="CompilationUnits">CompilationUnit</element>
		<element default="visible" description="Namespaces">NamespacePart</element>
		<element default="visible" description="Variables">MemberVariable</element>
		<element default="visible" description="Methods">RegularMethod</element>
		<element default="visible" description="Events">Event</element>
		<element default="visible" description="Indexers">Indexer</element>	
		<element default="visible" description="Properties">Property</element>	
		<element default="visible" description="Inner Classes">CSharpInnerClass</element>	
		<element default="visible" description="Inner Delegates">CSharpInnerDelegate</element>
		<element default="visible" description="Inner Interfaces">CSharpInnerInterface</element>
		<element default="visible" description="Interfaces">CSharpInterface</element>
		<element default="visible" description="Destructors">Destructor</element>			
	</outline>
</presentationmodel>